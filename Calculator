import sympy

class ComplicatedCalculator:
    def __init__(self):
        pass

    def add(self, a, b):
        return a + b

    def subtract(self, a, b):
        return a - b

    def multiply(self, a, b):
        return a * b

    def divide(self, a, b):
        return a / b

    def power(self, a, b):
        return a ** b

    def square_root(self, a):
        return sympy.sqrt(a)

    def logarithm(self, a, b):
        return sympy.log(a, b)

    def sine(self, a):
        return sympy.sin(a)

    def cosine(self, a):
        return sympy.cos(a)

    def tangent(self, a):
        return sympy.tan(a)

    def solve_integral(self, function, variable, limits):
        return sympy.integrate(function, (variable, limits[0], limits[1]))

    def solve_derivative(self, function, variable):
        return sympy.diff(function, variable)

if __name__ == "__main__":
    calc = ComplicatedCalculator()
    result = calc.solve_derivative('x**2', 'x')
    print("Result:", result)
